{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "development=true expo start",
    "android": "development=true expo start --android",
    "ios": "development=true expo start --ios",
    "web": "development=true expo start --web",
    "hybrid": "development=true expo start --android --web",
    "test": "jest",
    "refresh": "adb shell am force-stop host.exp.exponent",
    "debug": "adb shell input keyevent 82",
    "stage": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo publish -c --release-channel staging",
    "compile:android": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo build:android",
    "compile:ios": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo build:ios",
    "compile:web": "export $(grep '^[a-zA-Z_]*=.*' .env.production | xargs) && expo build:web",
    "publish": "export $(grep '^[a-zA-Z_]*=.*' .env | xargs) && expo publish && expo build:web",
    "bust": "npm cache clean --force && rm -fr $TMPDIR/metro* && watchman watch-del-all",
    "reset": "npm run bust && rm package-lock.json && rm -rf node_modules && npm i"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|redux-persist-expo-securestore)"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.10.5",
    "@sentry/browser": "^5.19.2",
    "babel-preset-expo": "^8.2.3",
    "base-64": "^0.1.0",
    "expo": "^38.0.8",
    "expo-cli": "^3.22.3",
    "expo-constants": "~9.1.1",
    "expo-firebase-analytics": "~2.4.1",
    "expo-image-manipulator": "~8.2.1",
    "expo-image-picker": "~8.3.0",
    "expo-network": "~2.2.1",
    "expo-permissions": "~9.0.1",
    "expo-random": "~8.2.1",
    "expo-screen-orientation": "~1.1.1",
    "expo-secure-store": "~9.0.1",
    "expo-updates": "~0.2.11",
    "firebase": "7.16.0",
    "history": "^5.0.0",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-gesture-handler": "~1.6.1",
    "react-native-paper": "^3.10.1",
    "react-native-web": "~0.11.7",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-native": "^5.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-expo-securestore": "^2.0.0",
    "redux-promise-middleware": "^6.1.2",
    "sentry-expo": "^2.1.2",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "jest": "^26.1.0",
    "jest-expo": "^38.0.2",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "^16.13.1",
    "redux-logger": "^3.0.6"
  },
  "private": true
}
