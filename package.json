{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "development=true expo start",
    "android": "development=true expo start:android",
    "ios": "development=true expo start:ios",
    "web": "development=true expo start:web",
    "hybrid": "development=true expo start --android --web",
    "eject": "expo eject",
    "test": "jest",
    "refresh": "adb shell am force-stop host.exp.exponent",
    "debug": "adb shell input keyevent 82",
    "stage": "expo publish -c --release-channel staging"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|redux-persist-expo-securestore)"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@sentry/browser": "^5.15.4",
    "babel-preset-expo": "^8.1.0",
    "expo": "^37.0.3",
    "expo-analytics-amplitude": "~8.1.0",
    "expo-cli": "^3.17.11",
    "expo-permissions": "~8.1.0",
    "expo-secure-store": "~8.1.0",
    "history": "^4.10.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.0.tar.gz",
    "react-native-paper": "^3.6.0",
    "react-native-web": "^0.11.7",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-router-native": "^5.1.2",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-persist-expo-securestore": "^2.0.0",
    "redux-promise-middleware": "^6.1.2",
    "sentry-expo": "^2.0.3",
    "expo-constants": "~9.0.0",
    "react-native-gesture-handler": "~1.6.0",
    "expo-screen-orientation": "~1.0.0"
  },
  "devDependencies": {
    "jest": "^25.1.0",
    "jest-expo": "^37.0.0",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "^16.13.1",
    "redux-logger": "^3.0.6"
  },
  "private": true
}
